{
  "compilerOptions": {
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@test/*": [
        "test/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}